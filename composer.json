{
    "name": "shippii-tech/php-sdk",
    "license": "MIT",
    "description": "SDK For shippii.com",
    "type": "library",
    "authors": [
        {
            "name": "Stoyan Todorov",
            "email": "s.todorov@vconnect.dk"
        }
    ],
    "require": {
        "php": "~8.1.0 | ~8.2.0 | ~8.3.0",
        "ext-json": "*",
        "cuyz/valinor": "^1.13",
        "guzzlehttp/guzzle": "^7.5.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "phpro/grumphp": "^2.8",
        "phpstan/phpstan": "^1.12",
        "symfony/var-dumper": "^6.2",
        "yieldstudio/grumphp-laravel-pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shippii\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
